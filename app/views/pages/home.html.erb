<div id="slider">
  <div class="large-12 columns">
  
    <div class="panel text-box">
        
        <div class="text">
          <br>
          <h4>Send me a text!</h4>
          <p>I'd love to hear from you, the modern way!</p>
        
    
          <%= form_for Message.new do |f| %>
            <%= f.label "Your Name" %>
            <%= f.text_field :name %>
            <%= f.label "Your Number" %>
            <%= f.text_field :number %>
            <%= f.label "Message" %>
            <%= f.text_area :text_message %>
            <%= f.submit "Sent it!", class:"submit button" %>
          <% end %>
       </div>
    </div>
    
  </div>
</div>

 

